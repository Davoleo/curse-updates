datasource db {
    provider = "sqlite"
    url = "file:../data.db"
}

generator client {
    provider = "prisma-client-js"
}

model ServerConfig {
    serverId String @id
    serverName String
    announcementConfigs AnnouncementsConfig[]
    projects CachedProject[] @relation(references: [projectId])
}

model AnnouncementsConfig {
    id Int @id @default(autoincrement())
    serverId String
    server ServerConfig @relation(fields: [serverId], references: [serverId])
    channel String?
    message String?
}

model CachedProject {
    projectId Int @id
    slug String @unique
    version String
    subscribedGuilds ServerConfig[] @relation(references: [serverId])
}

